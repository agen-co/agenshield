{
  "$schema": "https://mintlify.com/docs.json",
  "name": "AgenShield",
  "description": "Security daemon protecting AI agents from malicious skills",
  "appearance": {
    "default": "system",
    "strict": false
  },
  "theme": "mint",
  "colors": {
    "primary": "#3b82f6",
    "light": "#60a5fa",
    "dark": "#2563eb"
  },
  "metadata": {
    "og:site_name": "AgenShield Documentation",
    "og:locale": "en_US"
  },
  "navbar": {
    "links": [
      {
        "href": "https://github.com/agen-co/agenshield",
        "icon": "github",
        "label": "GitHub"
      }
    ],
    "primary": {
      "href": "https://github.com/agen-co/agenshield",
      "type": "github"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/agen-co/agenshield",
      "x": "https://x.com/agen_co"
    }
  },
  "search": {
    "prompt": "Search AgenShield docs..."
  },
  "styling": {
    "codeblocks": {
      "theme": {
        "dark": "dark-plus",
        "light": "snazzy-light"
      }
    }
  },
  "versions": ["v0.1"],
  "navigation": {
    "dropdowns": [
      {
        "dropdown": "Documentation",
        "icon": "book-open",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "docs/shield/get-started/welcome"
            ]
          }
        ]
      }
    ]
  }
}
